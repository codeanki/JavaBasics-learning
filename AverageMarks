/*
Sample Input:
Line1:Name(SingleCharacter)
Line2:Marks Obtained in 3 tests separated by single space

Sample Output:
First line of output prints the name
Second line prints the average marks
*/

import java.util.Scanner;
public class Solution{
   public static void main(String[] args){
     Scanner s = new Scanner(System.in);
     char ch = s.next().charAt(0):
     int m1 = s.nextInt();
     int m2 = s.nextInt();
     int m3 = s.nextInt();
     int average = (m1 + m2 + m3)/3;
     System.out.println(ch);
     System.out.println(average);
     }
}
