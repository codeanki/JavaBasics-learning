/* 
Sample Input:
num1: Take the first number as input from user
num2: Take the second number as input from user

Sample Output:
sum: Print the sum of two numbers as output
*/

import java.util.Scanner;
public class Solution{
   public static void main(String[] args){
     Scanner s = new Scanner(System.in);
     int num1 = s.nextInt();
     int num2 = s.nextInt();
     int sum = num1 + num2;
     System.out.println("Sum is" + sum);
     }
}
