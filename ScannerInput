/*
Sample Input:
100HelloWorld
Sample Output:
100
HelloWorld
*/

import java.util.Scanner;
public class ScannerDemo{
   public static void main(String[] args){
     Scanner s = new Scanner(System.in);
     int a = s.nextInt();
     String str = s.nextLine();
     System.out.println(a);
     System.out.println(str);
     }
}
